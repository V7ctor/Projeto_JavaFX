      <Label alignment="CENTER" prefHeight="32.0" prefWidth="578.0" text="Registros Departamento" textFill="#49466f">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
         <VBox.margin>
            <Insets left="15.0" right="15.0" />
         </VBox.margin></Label>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
        <items>
          <Button mnemonicParsing="false" text="Novo" />
        </items>
      </ToolBar>
      <TableView>
        <columns>
          <TableColumn prefWidth="75.0" text="ID" />
          <TableColumn prefWidth="75.0" text="Nome" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
